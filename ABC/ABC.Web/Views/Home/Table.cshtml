
@{
    ViewBag.Title = "Table";
    int x = 9;
    int y = 11;
    string[][] data = new string[x][];
    for (int i = 0; i < x; i++)
    {
        data[i] = new string[y];
    }
    data[0][0]= "0";
    data[0][1]= "0";
    data[1][3]= "0";
    data[1][4]= "0";
    data[2][3]= "0";
    data[2][6]= "0";
    data[3][5]= "0";
    data[3][6]= "0";
    data[4][2]= "0";
    data[4][3]= "0";
    data[4][4]= "0";
    data[4][5]= "0";
    data[4][6]= "0";
    data[6][3]= "0";
    data[7][1]= "0";
    data[7][5]= "0";
    data[8][7]= "0";
}
<style type="text/css">
    table {
        border-collapse: collapse;
        text-align: center;
        border: solid 1px #000;
        margin: auto;
    }

    td {
        border: solid 1px #000;
        text-align: center;
    }

    input {
        width: 50px;
        height: 50px;
        text-align: center;
    }
</style>

<table>
    @for (int i = 0; i < x; i++)
    {
        <tr>
            @for (int j = 0; j < y; j++)
            {
                <td><input type="text" value="@data[i][j]" /></td>
            }
        </tr>
    }
</table>